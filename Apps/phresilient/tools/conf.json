{
    "actions": [
        {
            "action": "test connectivity",
            "output": null,
            "endpoint": "\"/orgs/{org_id}/users/{id}\".format(**param)",
            "method": "get"
        },
        {
            "action": "list tickets",
            "description": "List all incidents",
            "parameters": [
                { "name": "want_closed", "data_type": "boolean", "default": true, "description": "Also returns closed incidents. Default is true" }
            ],
            "endpoint": "\"/orgs/{org_id}/incidents}\".format(**param)",
            "method": "get"
        },
        {
            "action": "get ticket",
            "description": "Get incident details by id",
            "parameters": [
                { "name": "incident_id", "required": true, "primary": true, "description": "ID of incident to retrieve", "contains": ["ticketid"] }
            ],
            "endpoint": "\"/orgs/{org_id}/incidents/{incidentID}\".format(**param)",
            "method": "get"
        },
        {
            "action": "create ticket",
            "description": "Create new incident",
            "type": "generic",
            "parameters": [
                { "name": "incident_name", "required": true, "primary": true, "description": "Name of incident" },
                { "name": "incident_description", "required": true, "description": "Short description of incident" },
                { "name": "FullIncidentDataDTO", "description": "Incident data as JSON String, format is FullIncidentDataDTO data type from API" },
                { "name": "want_full_data", "data_type": "boolean", "default": true, "description": "Returns full incident instead of summary. Default is true" },
                { "name": "want_tasks", "data_type": "boolean", "description": "Also returns associated tasks. Default is false" }
            ],
            "endpoint": "\"/orgs/{org_id}/incidents\".format(**param)",
            "method": "put"
        },
        {
            "action": "update ticket",
            "description": "Update existing incident",
            "type": "generic",
            "parameters": [
                { "name": "incident_id", "required": true, "primary": true, "description": "ID of incident to update", "contains": ["ticketid"] },
                { "name": "FullIncidentDataDTO", "required": true, "description": "Incident data as JSON String, format is FullIncidentDataDTO data type from API. This data should first be retrieved from Resilient and then modified with the desired changes" }
            ],
            "endpoint": "\"/orgs/{org_id}/incidents\".format(**param)",
            "method": "put"
        },
        {
            "action": "search tickets",
            "description": "Submit search query for incidents",
            "parameters": [
                { "name": "handle_format_is_name", "data_type": "boolean", "default": true, "description": "Treat handles as name. Default is true" },
                { "name": "QueryDTO", "description": "Query data as JSON String, format is QueryDTO data type from API" },
                { "name": "add_condition_all_active_tickets", "data_type": "boolean", "description": "Filters for active tickets" },
                { "name": "add_condition_closed_in_last_24_hours", "data_type": "boolean", "description": "Filters for tickets closed in last 24 hours" },
                { "name": "add_condition_created_in_last_24_hours", "data_type": "boolean", "description": "Filters for tickets created in last 24 hours" },
                { "name": "1st_condition_field_name", "description": "The field name in the condition" },
                { "name": "1st_condition_field_value", "description": "The value used for comparison" },
                { "name": "1st_condition_value_is_datetime", "data_type": "boolean", "description": "The value is a datetime; convert to Resilient internal representation" },
                { "name": "1st_condition_comparison_method", "description": "The method used to compare the value, [equals, gt, lte]. Look up MethodName datatype in the api" },
                { "name": "2nd_condition_field_name", "description": "The field name in the condition" },
                { "name": "2nd_condition_field_value", "description": "The value used for comparison" },
                { "name": "2nd_condition_value_is_datetime", "data_type": "boolean", "description": "The value is a datetime" },
                { "name": "2nd_condition_comparison_method", "description": "The method used to compare the value" },
                { "name": "3rd_condition_field_name", "description": "The field name in the condition" },
                { "name": "3rd_condition_field_value", "description": "The value used for comparison" },
                { "name": "3rd_condition_value_is_datetime", "data_type": "boolean", "description": "The value is a datetime" },
                { "name": "3rd_condition_comparison_method", "description": "The method used to compare the value" },
                { "name": "4th_condition_field_name", "description": "The field name in the condition" },
                { "name": "4th_condition_field_value", "description": "The value used for comparison" },
                { "name": "4th_condition_value_is_datetime", "data_type": "boolean", "description": "The value is a datetime" },
                { "name": "4th_condition_comparison_method", "description": "The method used to compare the value" },
                { "name": "5th_condition_field_name", "description": "The field name in the condition" },
                { "name": "5th_condition_field_value", "description": "The value used for comparison" },
                { "name": "5th_condition_value_is_datetime", "data_type": "boolean", "description": "The value is a datetime" },
                { "name": "5th_condition_comparison_method", "description": "The method used to compare the value" }
            ],
            "output": "search tickets"
        },
        {
            "action": "list artifacts",
            "description": "List all artifacts for incident",
            "parameters": [
                { "name": "incident_id", "required": true, "primary": true, "description": "ID of incident", "contains": ["ticketid"] }
            ],
            "output": "artifacts"
        },
