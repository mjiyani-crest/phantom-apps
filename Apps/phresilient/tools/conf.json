                { "name": "handle_format_is_name", "data_type": "boolean", "default": true, "description": "Treat handles as name. Default is true" },
                { "name": "incident_id", "required": true, "primary": true, "description": "ID of incident", "contains": ["ticketid"] },
                { "name": "comment_id", "required": true, "primary": true, "description": "ID of comment to retrieve", "contains": ["commentid"] }
            ],
            "output": "comments"
        },
        {
            "action": "create comment",
            "description": "create new comment",
            "parameters": [
                { "name": "handle_format_is_name", "data_type": "boolean", "default": true, "description": "Treat handles as name. Default is true" },
                { "name": "incident_id", "required": true, "primary": true, "description": "ID of incident", "contains": ["ticketid"] },
                { "name": "parent_id", "primary": true, "description": "The ID of the initial comment if posting a reply", "contains": ["commentid"] },
                { "name": "text", "description": "comment as text" },
                { "name": "IncidentCommentDTO", "description": "comment data as JSON String, format is IncidentCommentDTO data type from API" }
            ],
            "type": "generic",
            "output": "comments"
        },
        {
            "action": "update comment",
            "description": "update existing comment",
            "parameters": [
                { "name": "handle_format_is_name", "data_type": "boolean", "default": true, "description": "Treat handles as name. Default is true" },
                { "name": "incident_id", "required": true, "primary": true, "description": "ID of incident", "contains": ["ticketid"] },
                { "name": "comment_id", "required": true, "primary": true, "description": "ID of comment to retrieve", "contains": ["commentid"] },
                { "name": "IncidentCommentDTO", "required": true, "description": "comment data as JSON String, format is IncidentCommentDTO data type from API" }
            ],
            "type": "generic",
            "output": "comments"
        },
