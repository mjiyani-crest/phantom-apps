    "output": {
        "default":[
            { "path": "id", "contains": ["ticketid"] },
            { "path": "name" },
            { "path": "create_date" },
            { "path": "description" },
            { "path": "creator.fname" },
            { "path": "creator.lname" },
            { "path": "creator.email" },
            { "path": "creator.phone" },
            { "path": "creator.cell" }
        ],
        "search tickets": [
            { "path": "id", "contains": ["ticketid"] },
            { "path": "name" },
            { "path": "create_date" },
            { "path": "description" },
            { "path": "severity_code.name", "column_name": "severity" },
            { "path": "plan_status", "column_name": "status" }
        ],
        "artifacts": [
            { "path": "inc_id", "column_name": "incident id", "contains": ["ticketid"] },
            { "path": "id", "column_name": "artifact id", "contains": ["artifactid"] },
            { "path": "type" },
            { "path": "value" },
            { "path": "description" },
            { "path": "inc_name", "column_name": "incident name" }
        ],
        "comments": [
            { "path": "inc_id", "column_name": "incident id", "contains": ["ticketid"] },
            { "path": "id", "column_name": "comment id", "contains": ["commentid"] },
            { "path": "text" },
            { "path": "inc_name", "column_name": "incident name" }
        ],
        "json": [
        ]
    },
    "render": {
        "default": {
            "title": "Resilient Incident"
        }
    },
    "status": [
        { "code": 200, "success": true,  "progress": "Test connectivity successful", "response": false },
        { "code": 400, "success": false, "progress": "Bad request", "response": false },
        { "code": 401, "success": false, "progress": "Unauthorized - most commonly, the provided session ID is invalid", "response": false },
        { "code": 403, "success": false, "progress": "Forbidden - most commonly, user authentication failed", "response": false },
        { "code": 404, "success": false, "progress": "Object not found", "response": false },
        { "code": 409, "success": false, "progress": "Conflicting PUT operation", "response": false },
        { "code": 500, "success": false, "progress": "Internal error", "response": false },
        { "code": 503, "success": false, "progress": "Service unavailable - usually related to LDAP not being accessible", "response": false }
    ]
}
