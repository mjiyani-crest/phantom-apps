                { "name": "IncidentArtifactDTO", "description": "Artifact data as JSON String, format is IncidentArtifactDTO data type from API" },
                { "name": "type", "values": ["url", "domain", "file"], "description": "Type of artifact. Known values are; url(3), domain(2), file(13)" },
                { "name": "value", "description": "artifact value field" },
                { "name": "description", "description": "short description of artifact" }
            ],
            "type": "generic",
            "output": "artifacts"
        },
        {
            "action": "update artifact",
            "description": "update existing artifact",
            "parameters": [
                { "name": "incident_id", "required": true, "primary": true, "description": "ID of incident", "contains": ["ticketid"] },
                { "name": "artifact_id", "required": true, "primary": true, "description": "ID of artifact to update", "contains": ["artifactid"] },
                { "name": "IncidentArtifactDTO", "required": true, "description": "Artifact data as JSON String, format is IncidentArtifactDTO data type from API" }
            ],
            "type": "generic",
            "output": "artifacts"
        },
        {
            "action": "list comments",
            "description": "list all comment for incident",
            "parameters": [
                { "name": "handle_format_is_name", "data_type": "boolean", "default": true, "description": "Treat handles as name. Default is true" },
                { "name": "incident_id", "required": true, "primary": true, "description": "ID of incident", "contains": ["ticketid"] }
            ],
            "output": "comments"
        },
        {
            "action": "get comment",
            "description": "get comment details by incident and comment id",
